    "is-electron": "^2.2.1",
    "logfmt": "^1.3.2",
    "react": "^18.1.0",
    "react-bootstrap": "^2.0.2",
    "react-dom": "^18.1.0",
    "react-editext": "^4.2.1",
    "react-outside-click-handler": "^1.3.0",
    "react-redux": "^8.0.1",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "react-toastify": "^9.0.1",
    "regenerator-runtime": "^0.13.9",
    "shelljs": "^0.8.5",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.3",
